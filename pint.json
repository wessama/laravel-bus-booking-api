{
    "preset": "laravel",
    "rules": {
        "no_unused_imports": true,
        "ordered_imports" : {
            "sort_algorithm": "alpha"
        },
        "concat_space": {
            "spacing" : "one"
        },
        "return_type_declaration" : {
            "space_before" : "one"
        }
    }
}
